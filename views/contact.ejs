        rel="stylesheet">





    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;

        }

        body {
            width: 100%;
            height: 100vh;
            display: grid;
            align-items: center;
            /* background: linear-gradient(#2067ea, rgb(157, 0, 255), rgb(34, 211, 34)); */

            /* font-family: 'Roboto', sans-serif; */
            font-family: 'Poppins', sans-serif;
            /* font-family: 'Roboto', sans-serif; */
        }

        .contact-in {
            width: 70%;
            height: 70vh;
            margin-top: 90px;
            margin-bottom: 20px;
            margin-left:15%;
            /* margin: auto; */
            text-align: center;
            display: flex;
            flex-wrap: wrap;
            padding: 10px;
            border-radius: 10px;
            background: #fff;
            box-shadow: 0px 0px 10px 0px rgb(164, 227, 165);

        }

        .contact-map {
            width: 100%;
            height: auto;
            flex: 50%;

        }

        .contact-form {
            width: 100%;
            /* height: auto; */
            flex: 50%;
            padding: 30px;
            text-align: center;
        }

        .contact-form,
        iframe {
            width: 100%;
            height: 100%;
        }

        .contact-form h1 {
            margin: 10px;
        }

        .contact-text {
            width: 100%;
            height: 40px;
            color: #000;
            border: 1px solid #58d778;
            border-radius: 10px;
            outline: none;
            margin-bottom: 20px;
            padding: 15px;


        }

        .contact-textarea {
            width: 100%;
            height: 130px;
            color: #000;
            border: 1px solid #78d476;
            border-radius: 6px;
            outline: none;
            margin-bottom: 20px;
            padding: 15px;
            /* font-family: 'Roboto', sans-serif; */
            font-family: 'Poppins', sans-serif;
            /* font-family: 'Roboto', sans-serif; */
        }

        .contact-textarea::placeholder {
            color: #aaa;

        }

        .contact-btn {
            width: 100%;
            border: none;
            outline: none;
            border-radius: 10px;
            background: #15ac59;
            color: #fff;
            text-transform: uppercase;
            padding: 10px 0px;
            cursor: pointer;
            font-size: 15px;

        }
    </style>



    <div class="contact-in">
        <div class="contact-map">
            <iframe
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3605.863566605172!2d81.89938477410486!3d25.342358825808194!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x39853534ffffffff%3A0xbce4c1b3c3577b37!2sUnited%20Institute%20of%20Management%20-%20Allahabad!5e0!3m2!1sen!2sin!4v1680976484104!5m2!1sen!2sin"
                width="100%" height="auto" style="border:0;" allowfullscreen="" loading="lazy"
                referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
        <div class="contact-form">
            <form action="">

                <h1>Contact Us</h1>

                <input type="text" placeholder="Name" class="contact-text">


                <input type="email" name="" placeholder="Email" class="contact-text">

                <textarea name="" id="" cols="30" rows="10" placeholder="Message" class="contact-textarea"></textarea>
              

                <input type="submit" name="Submit" class="contact-btn">
            </form>
        </div>
    </div>
    <%- include('partials/footer.ejs') %>
</body>
</html>

